{
  "name": "rhino-digital-graphql",
  "version": "1.0.0",
  "description": "Repositorio con el c√≥digo del backend del proyecto web rhino digital",
  "main": "src/server.ts",
  "repository": "https://github.com/Neider-Urbano/rhino-digital-graphql.git",
  "author": "Neider Urbano <jjuly8julianur@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=15.14.0",
    "npm": ">=8.3.1",
    "yarn": ">=1.22.10 <2"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --ignore-watch node_modules src/server.ts",
    "build": "tsc --build tsconfig.json",
    "build:watch": "tsc --build tsconfig.json --watch",
    "compile": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "prepare": "husky install",
    "lint": "eslint -c .eslintrc.json --ext .ts,.tsx ./src",
    "all:checks": "concurrently --names \"lint,test,build,compile\" \"yarn lint\" \"yarn test\" \"yarn build\" \"yarn compile\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn compile && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "yarn lint --fix",
      "prettier --write"
    ]
  },
  "resolutions": {
    "lodash": "^4.17.21",
    "node-forge": "^1.3.0",
    "ansi-regex": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
