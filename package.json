{
  "name": "rhino-digital-graphql",
  "version": "1.0.0",
  "description": "Rhino Digital platform GQL",
  "main": "src/server.ts",
  "engines": {
    "node": ">=15.14.0",
    "npm": ">=8.3.1",
    "yarn": ">=1.22.10"
  },
  "scripts": {
    "tsc": "tsc",
    "prod": "tsc && node build/server.js",
    "build": "tsc --build tsconfig.json",
    "build:watch": "tsc --build tsconfig.json --watch",
    "dev": "ts-node-dev src/server.ts",
    "start": "node build/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint -c .eslintrc.json --ext .ts,.tsx ./src",
    "compile": "tsc --noEmit",
    "all:checks": "concurrently --names \"lint,test,compile\" \"yarn lint\" \"yarn test\" \"yarn compile\""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\""
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neider-Urbano/rhino-digital-graphql.git"
  },
  "keywords": [
    "rhino-digital"
  ],
  "author": "Rhino Digital <jjuly8julianur@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Neider-Urbano/rhino-digital-graphql/issues"
  },
  "homepage": "https://github.com/Neider-Urbano/rhino-digital-graphql#readme",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.59.11",
    "babel-jest": "^29.5.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "*"
  },
  "resolutions": {
    "lodash": "^4.17.21",
    "node-forge": "^1.3.0",
    "ansi-regex": "^3.0.1"
  }
}
